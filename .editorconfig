# ADF .editorConfig file
# v 1.40

root = true

[*]
end_of_line = crlf
insert_final_newline = true
charset = utf-8
indent_style = tab
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.css]
end_of_line = lf
insert_final_newline = false

[*.html]
indent_style = space
indent_size = 2